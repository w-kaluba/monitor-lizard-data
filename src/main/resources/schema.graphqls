scalar Long

type Query {
    candlesticksFromTime(time: Long): Candlestick
    symbolByName(name: String): Symbol
}

type Candlestick {
    openTime: Long
    open: String
    high: String
    low: String
    close: String
    volume: String
}

type Symbol {
    name: String
    change: String
    high: String
    low: String
    volume: String
}
